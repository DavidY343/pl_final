(defun fibonacci (a b n)
(setq fibonacci_retorno 0)
(if (< ( n ) 2)
(setf b ( b ))
(setf n ( fibonacci ( b ) (+ ( a ) ( b )) (- ( n ) 1) ))
)
return-from fibonacci ( fibonacci_retorno )
)
(defun main ()
(setq main_resultado 0)
(setq main_i 0)
(print "Sucesion de Fibonacci")
(setf main_i 0)
(loop while (< ( main_i ) 47) do 
(setf main_i ( fibonacci 0 1 ( main_i ) ))
(prin1 ( main_i )) (prin1  ) (prin1 ( main_resultado ))
(print " ")
(setf main_i (+ ( main_i ) 1)))
)
 (main)
