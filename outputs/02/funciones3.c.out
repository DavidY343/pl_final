(defun cuadrado (a)
(setq cuadrado_c 0)
(setf cuadrado_c (* a a))
(return-from cuadrado cuadrado_c)
)
(defun main ()
(setq main_a 0)
(setq main_c 0)
(setf main_a 7)
(print "El cuadrado de ")
(prin1 main_a)
(setf main_c (cuadrado main_a))
(print " es ")
(prin1 main_c)
(setf main_a 12)
(prin1 "El cuadrado de ") (prin1 main_a) (prin1 " es: ") (prin1 (cuadrado main_a))
(print " ")
)
 (main)
